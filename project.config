[access "refs/heads/*"]
	abandon = group Change Owner
	abandon = group ops-sysd-maintainers
	label-Code-Review = -2..+2 group ops-sysd-maintainers
	label-Code-Review = block group Change Owner
	label-Workflow = -1..+1 group Change Owner
	pushTag = group ops-sysd-maintainers
	pushSignedTag = group ops-sysd-maintainers

[submit]
	mergeContent = true
	action = rebase if necessary
